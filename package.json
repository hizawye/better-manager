{
  "name": "better-manager",
  "version": "0.1.0",
  "description": "A lightweight AI account manager and protocol proxy",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/index.ts",
    "dev:frontend": "vite --config frontend/vite.config.ts",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "vite build --config frontend/vite.config.ts",
    "start": "node dist/index.js",
    "preview": "vite preview --config frontend/vite.config.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "keywords": [
    "ai",
    "proxy",
    "gemini",
    "claude",
    "openai"
  ],
  "author": "nagara",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.38.0",
    "express": "^4.21.0",
    "framer-motion": "^11.18.0",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.468.0",
    "open": "^10.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.3.4",
    "react-router-dom": "^7.1.3",
    "recharts": "^2.15.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "daisyui": "^4.12.24",
    "drizzle-kit": "^0.30.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^7.0.4"
  }
}
